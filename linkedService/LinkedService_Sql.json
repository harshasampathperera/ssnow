{
	"name": "LinkedService_Sql",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"ServerName": {
				"type": "string"
			},
			"dbName": {
				"type": "string"
			},
			"UserName": {
				"type": "string"
			},
			"Password": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Data Source=@{linkedService().ServerName};Initial Catalog=@{linkedService().dbName};User ID=@{linkedService().UserName};Password=@{linkedService().Password}"
		},
		"connectVia": {
			"referenceName": "IROnpromise",
			"type": "IntegrationRuntimeReference"
		}
	}
}